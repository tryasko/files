apiVersion: automation.cloudbees.io/v1alpha1
kind: workflow
name: Sleep-60
on:
  push:
    branches:
      - "**"
jobs:
  build:
    steps:
      - name: Run Sleep-60
        uses: docker://golang:1.20.3-alpine3.17
        shell: sh
        run: |
          sleep 60
          echo "Sleep-60 is done"
