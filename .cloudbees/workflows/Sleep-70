apiVersion: automation.cloudbees.io/v1alpha1
kind: workflow
name: Sleep-70
on:
  push:
    branches:
      - "**"
jobs:
  build:
    steps:
      - name: Run Sleep-70
        uses: docker://golang:1.20.3-alpine3.17
        shell: sh
        run: |
          sleep 70
          echo "Sleep-70 is done"
